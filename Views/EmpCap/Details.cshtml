@model RHCRUD.Models.EmpCap

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles del registro</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Nombre de la capacitación
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdCapacitacionNavigation.NombreCapacitacion)
        </dd>
        <dt class="col-sm-2">
            Nombre del empleado
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RutEmpleadoNavigation.NombreEmpleado)
        </dd>
        <dt class="col-sm-2">
            Apellido paterno
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RutEmpleadoNavigation.ApPaternoEmpleado)
        </dd>
        <dt class="col-sm-2">
            Apellido materno
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RutEmpleadoNavigation.ApMaternoEmpleado)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.IdEmpeCap">Modificar</a> |
    <a class="btn btn-primary" asp-action="Index">Volver a la lista</a>
</div>
